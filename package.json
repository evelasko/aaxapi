{
  "name": "aaxapi",
  "version": "1.0.0",
  "description": "AliciAlonso GraphQL API Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/evelasko/aaxapi.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "env-cmd -f ./config/test.env jest --watch",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js --exec babel-node",
    "dex": "env-cmd -f ./config/dex.env nodemon src/index.js --exec babel-node",
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "get-schema-dev": "graphql get-schema -p prisma --dotenv config/dev.env",
    "get-schema-dex": "graphql get-schema -p prisma --dotenv config/dex.env",
    "get-tokens": "env-cmd -f ./config/dex.env node ./prisma/scripts/prismaToken.js",
    "seed-users": "env-cmd -f ./config/dex.env node ./prisma/scripts/seedUsers.js --exec babel-node",
    "seed": "env-cmd -f ./config/dex.env node ./prisma/scripts/seed.js",
    "cat": "env-cmd -f ./config/dex.env node ./prisma/scripts/seedCongressProductsAndDiscounts.js"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globaTeardown.js",
    "verbose": false
  },
  "author": "Enrique Velasco",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^1.0.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.14.0",
    "connect-redis": "^4.0.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.0.1",
    "expo-server-sdk": "^3.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.2",
    "formidable": "^1.2.1",
    "graphql-shield": "^6.1.0",
    "graphql-yoga": "^1.18.3",
    "handlebars": "^4.1.2",
    "html-entities": "^1.2.1",
    "http-proxy-middleware": "^0.20.0",
    "ioredis": "^4.9.0",
    "is-html": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.4",
    "node-redsys-api": "0.0.5",
    "nodemailer": "^6.3.0",
    "nodemailer-mailgun-transport": "^1.4.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "order-id": "^1.1.0",
    "pg": "^7.12.1",
    "prisma-binding": "^2.3.10",
    "prisma-client-lib": "^1.34.8",
    "qrcode": "^1.4.1",
    "qs": "^6.8.0",
    "shortid": "^2.2.14",
    "uuid": "^3.3.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "apollo-boost": "^0.4.4",
    "cross-fetch": "^3.0.4",
    "graphql": "^14.2.1",
    "jest": "^24.1.0",
    "kexec": "^3.0.0",
    "nodemon": "^1.18.9"
  }
}
