{
  "name": "aaxapi",
  "version": "1.0.0",
  "description": "AliciAlonso GraphQL API Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/evelasko/aaxapi.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "env-cmd ./config/test.env jest --watch",
    "dev": "env-cmd ./config/dev.env nodemon src/index.js --exec babel-node",
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globaTeardown.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "adm-zip": ">=0.4.11",
    "apollo-boost": "^0.1.22",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.0",
    "env-cmd": "^8.0.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql-cli": "^3.0.3",
    "graphql-shield": "^5.1.0",
    "graphql-yoga": "^1.16.9",
